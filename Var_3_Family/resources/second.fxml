<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="463.0" prefWidth="613.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoexam.SecondController">
    <Label text="ФИО:" />
    <TextField fx:id="fioField" promptText="ФИО" />

    <Label text="Начало рабочего стажа:" />
    <DatePicker fx:id="startDatePick"/>

    <Label text="Текущая должность:" />
    <TextField fx:id="positionField" promptText="Название должности" />

    <Label text="Место работы:" />
    <TextField fx:id="orgField" promptText="Название места работы" />

    <Label text="Оклад:" />
    <TextField fx:id="salaryField" promptText="Суммарный оклад за месяц в руб." />


    <Button onAction="#saveChanges" text="Сохранить" />
</VBox>
