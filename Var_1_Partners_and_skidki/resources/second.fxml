<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="463.0" prefWidth="613.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoexam.SecondController">
   <VBox prefHeight="200.0" prefWidth="613.0">
      <children>
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefHeight="26.0" prefWidth="400.0" text="Тип партнера:">
            <graphic>
                <TextField fx:id="typeField" prefWidth="300.0" promptText="ОАО, ЗАО, ИП, ООО" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="30.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefWidth="600.0" text="Название компании партнера:">
            <graphic>
                <TextField fx:id="nameField" prefWidth="300.0" promptText="Название" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" text="ФИО директора:">
            <graphic>
                <TextField fx:id="directorField" maxWidth="350.0" prefWidth="300.0" promptText="ФИО" />
            </graphic>
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefWidth="600.0" text="Электронная почта:">
            <graphic>
                <TextField fx:id="emailField" prefWidth="300.0" promptText="Email" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefWidth="600.0" text="Номер телефона:">
            <graphic>
                <TextField fx:id="phoneField" prefWidth="300.0" promptText="Номер" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefWidth="600.0" text="Юридический Адрес:">
            <graphic>
                <TextField fx:id="addressField" prefWidth="300.0" promptText="Почтовый индекс, область, город, ул, № дома" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Label contentDisplay="RIGHT" graphicTextGap="8.0" prefWidth="400.0" text="ИНН:">
            <graphic>
                <TextField fx:id="innField" prefWidth="300.0" promptText="ИНН" />
            </graphic>
            <VBox.margin>
               <Insets left="20.0" top="15.0" />
            </VBox.margin>
         </Label>
      
          <Button alignment="BOTTOM_CENTER" contentDisplay="BOTTOM" onAction="#saveChanges" text="Сохранить">
            <VBox.margin>
               <Insets left="230.0" top="80.0" />
            </VBox.margin>
         </Button>
      </children>
      <padding>
         <Insets left="30.0" top="30.0" />
      </padding>
   </VBox>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
